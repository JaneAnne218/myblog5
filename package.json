{
  "name": "@jcamp/vitepress-blog-theme",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "^pnpm@8.1.0",
  "description": "VitePress easy to use blog theme",
  "author": "John Campion",
  "license": "MIT",
  "homepage": "https://github.com/jcamp-code/starter-vitepress-theme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcamp-code/starter-vitepress-theme.git"
  },
  "bugs": {
    "url": "https://github.com/jcamp-code/starter-vitepress-theme/issues"
  },
  "keywords": [
    "vite",
    "vue",
    "vitepress",
    "theme",
    "blog"
  ],
  "exports": {
    ".": {
      "types": "./dist/theme/index.d.ts",
      "default": "./dist/theme/index.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "default": "./dist/node/index.js"
    }
  },
  "main": "./dist/theme/index.js",
  "types": "./dist/theme/index.d.ts",
  "files": [
    "dist",
    "node.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "run-s clean dev:start",
    "dev:start": "run-p dev:tsc dev:copy",
    "dev:tsc": "tsc --sourcemap -w",
    "dev:copy": "node scripts/copy -w",
    "build": "run-s clean build:tsc build:copy",
    "build:tsc": "tsc && vue-tsc --declaration --emitDeclarationOnly",
    "build:copy": "node scripts/copy",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check --write .",
    "postinstall": "npx simple-git-hooks",
    "test": "vitest run",
    "release": "changelogen --release --push && pnpm publish",
    "release:minor": "changelogen --release --minor --push && pnpm publish",
    "release:major": "changelogen --release --major --push && pnpm publish",
    "release:patch": "changelogen --release --patch --push && pnpm publish",
    "demo": "run-s build demo:start",
    "demo:start": "run-p dev:start demo-dev",
    "demo-dev": "cd demo && pnpm dev",
    "demo-build": "run-p build demo-build-only",
    "demo-build-only": "cd demo && pnpm build",
    "demo-preview": "cd demo && pnpm preview"
  },
  "dependencies": {
    "@iconify-json/bx": "^1.1.6",
    "@iconify-json/carbon": "^1.1.16",
    "@iconify-json/fluent-emoji": "^1.1.12",
    "@iconify-json/heroicons-outline": "^1.1.6",
    "@iconify-json/logos": "^1.1.24",
    "@jcamp/tailwindcss-plugin-icons": "^0.5.0",
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.3.1",
    "vitepress": "1.0.0-alpha.64",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@jcamp/eslint-config": "0.6.1",
    "@types/node": "^18.15.10",
    "@vue/tsconfig": "^0.1.3",
    "changelogen": "^0.5.2",
    "chokidar": "3.5.3",
    "commitlint-config-unjs": "^1.0.0",
    "date-fns": "^2.29.3",
    "eslint": "^8.38.0",
    "fast-glob": "^3.2.12",
    "feed": "^4.2.2",
    "flexsearch": "^0.7.31",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "lint-staged": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rimraf": "5.0.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitepress": "1.0.0-alpha.64",
    "vitepress-plugin-search": "1.0.4-alpha.19",
    "vitest": "^0.29.8",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ],
      "allowedVersions": {
        "typescript": "5"
      }
    }
  }
}
